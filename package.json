{
    "name": "emeraldjava",
    "version": "1.0.0",
    "description": "This is the resume of emeraldjava.",
    "main": "index.js",
    "type": "module",
    "scripts": {
        "init": "resumed init",
        "clean": "del-cli --force ./out/**",
        "theme-onepage": "resumed --theme jsonresume-theme-onepage --output out/jsonresume-onepage.html",
        "theme-onepage-plus": "resumed --theme jsonresume-theme-onepage-plus",
        "theme-relaxed": "resumed --theme jsonresume-theme-relaxed",
        "theme-macchiato": "resumed -t jsonresume-theme-macchiato -o out/jsonresume-macchiato.html",
        "theme-actual": "resumed --theme jsonresume-theme-actual",
        "theme-actual-entropitor": "resumed --theme jsonresume-theme-actual-entropitor",
        "theme-actual-letter": "resumed --theme jsonresume-theme-actual-letter",
        "theme-kendall": "resumed --theme jsonresume-theme-kendall",
        "theme-jacrys": "resumed --theme jsonresume-theme-jacrys",
        "theme-paper": "resumed --theme jsonresume-theme-paper",
        "theme-paper-plus-plus": "resumed --theme jsonresume-theme-paper-plus-plus --output out/jsonresume-paper-plus-plus.html",
        "all": "npm-run-all clean theme-paper-plus-plus theme-onepage theme-macchiato",
        "pdf": "node resumed-as-pdf.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/emeraldjava/resume.git"
    },
    "keywords": [
        "resume"
    ],
    "author": "emeraldjava",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/emeraldjava/resume/issues"
    },
    "homepage": "https://github.com/emeraldjava/resume#readme",
    "dependencies": {
        "jsonresume-theme-actual": "1.2.2",
        "jsonresume-theme-actual-entropitor": "1.2.0",
        "jsonresume-theme-actual-letter": "0.1.6",
        "jsonresume-theme-direct": "^0.1.1",
        "jsonresume-theme-eloquent": "5.0.0",
        "jsonresume-theme-eloquent-mod": "^4.24.0",
        "jsonresume-theme-github": "^0.0.5",
        "jsonresume-theme-github2": "^0.0.8",
        "jsonresume-theme-jacrys": "1.0.4",
        "jsonresume-theme-kendall": "0.2.0",
        "jsonresume-theme-latte": "^1.0.2",
        "jsonresume-theme-macchiato": "^1.1.0",
        "jsonresume-theme-onepage": "^0.0.3",
        "jsonresume-theme-onepage-plus": "1.0.1",
        "jsonresume-theme-paper": "0.5.0",
        "jsonresume-theme-paper-plus-plus": "0.5.1",
        "jsonresume-theme-relaxed": "0.1.8",
        "jsonresume-theme-stackoverflow": "^2.0.2",
        "puppeteer": "^21.9.0",
        "resumed": "^3.0.1"
    },
    "devDependencies": {
        "del-cli": "^5.1.0",
        "npm-run-all": "^4.1.5"
    }
}
